<h1> New Student </h1>

<%= form_with  model: @student do |form| %>

<div>
<%= form.label :firstname %></br>
<%= form.text_field :firstname %></br>
</div>

<div>
<%= form.label :lastname %></br>
<%= form.text_field :lastname %></br>
</div>

<div>
<%= form.label :email %></br>
<%= form.text_field :email %></br>
</div>
<div>
<%= form.submit %>
</div>

<% end %>
